<h2>文章列表</h2>
<%= will_paginate @visitors%>
<table class="table table-hover">
  <thead>
    <tr>
      <th>标题</th>
      
      <th>发表时间</th>
    </tr>
  </thead>

  <tbody>
    <% @visitors.each do |visitor| %>
      <tr>
        <td><%= link_to visitor.title, visitor %></td>
        <td>Posted <%= time_ago_in_words(visitor.created_at) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
 <%= will_paginate %>